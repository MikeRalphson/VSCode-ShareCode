{
  "name": "sharecode",
  "version": "0.1.0",
  "publisher": "RolandGreim",
  "engines": {
    "vscode": "^0.10.10"
  },
  "license": "GPL-3.0",
  "displayName": "Share Code",
  "description": "Share your Code on Pastebin or GitHub Gist.",
  "categories": [
    "Other"
  ],
  "keywords": [
      "GitHub",
      "Pastebin",
      "Upload",
      "Share"
  ],
  "galleryBanner": {
    "color": "#008080",
    "theme": "light"
  },
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.shareCode",
        "title": "Share Code"
      }
    ],
    "configuration": {
        "type": "string",
        "title": "Share Code configuration",
        "properties": {
            "shareCode.pastebin.username": {
                "type": "string",
                "default": "",
                "description": "Cached pastebin username."
            },
            "shareCode.gist.username": {
                "type": "string",
                "default": "",
                "description": "Cached gist username."
            }
        }
    }
  },
  "activationEvents": [
    "onCommand:extension.shareCode"
  ],
  "dependencies": {
    "opn": "^4.0.1",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "icon": "images/sharecode.svg",
  "author": {
    "name": "Roland Greim - http://www.rolandgreim.de/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tigerxy/VSCode-ShareCode"
  },
  "bugs": {
    "url": "https://github.com/tigerxy/VSCode-ShareCode/issues"
  }
}
